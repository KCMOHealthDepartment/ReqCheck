<p id="notice"><%= notice %></p>

<h1>Listing Vaccines</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

<tbody>
  <% @vaccine_doses.each do |vaccine_dose| %>
  <tr>
<td><%= link_to 'Show', vaccine_dose %></td>
<td><%= link_to 'Edit', edit_vaccine_dose_path(vaccine_dose) %></td>
<td><%= link_to 'Destroy', vaccine_dose, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</tbody>
</table>

<br>

<%= link_to 'New Vaccine Dose', new_vaccine_dose_path %>
